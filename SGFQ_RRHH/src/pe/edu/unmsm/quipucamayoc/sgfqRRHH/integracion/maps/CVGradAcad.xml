<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="CVGradosAcad" >
<typeAlias alias="CVGradosAcadDTO" type="pe.edu.unmsm.quipucamayoc.sgfqRRHH.negocio.dto.CVGradosAcadDTO"/>
<resultMap class="CVGradosAcadDTO" id="rsCVGradAcad">
	<result property="cvDni" columnIndex="1"/>
	<result property="estudiosUnivInst" columnIndex="2"/>
	<result property="nombreUnivInst" columnIndex="3"/>
	<result property="fechaIni" columnIndex="4"/>
	<result property="fechaFin" columnIndex="5"/>
	<result property="nivelAcadLogr" columnIndex="6"/>
</resultMap>

<insert id="insertCVGradAcad" parameterClass="CVGradosAcadDTO">
INSERT INTO QPDATAGESTION.TB_CV_ACADEMICO
(C_CVDNI,T_ESTUDIOS_UNIV_INST,T_NOMBRE_UNIV_INST,F_FECHA_INI,F_FECHA_FIN,T_NIVEL_ACAD_LOGR)
VALUES(#cvDni#,#estudiosUnivInst#,#nombreUnivInst#,#fechaIni#,#fechaFin#,#nivelAcadLogr#)
</insert>

<delete id="deleteCVGradAcad" >
DELETE QPDATAGESTION.TB_CV_ACADEMICO WHERE C_CVDNI=#cvDni#
</delete>

<select id="selectCVGradAcad" resultMap="rsCVGradAcad">
SELECT C_CVDNI,T_ESTUDIOS_UNIV_INST,T_NOMBRE_UNIV_INST,F_FECHA_INI,F_FECHA_FIN,T_NIVEL_ACAD_LOGR
FROM QPDATAGESTION.TB_CV_ACADEMICO WHERE C_CVDNI=#cvDni# 
</select>

</sqlMap>