<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="CVExpLab" >
<typeAlias alias="CVExpLabDTO" type="pe.edu.unmsm.quipucamayoc.sgfqRRHH.negocio.dto.CVExpLabDTO"/>
<resultMap class="CVExpLabDTO" id="rsCVExpLab">
	<result property="cvDni" columnIndex="1"/>
	<result property="nomEmpresa" columnIndex="2"/>
	<result property="actividadEmpresa" columnIndex="3"/>
	<result property="sueldo" columnIndex="4"/>
	<result property="fechaIni" columnIndex="5"/>
	<result property="fechaFin" columnIndex="6"/>
</resultMap>

<insert id="insertCVExpLab" parameterClass="CVExpLabDTO">
INSERT INTO QPDATAGESTION.TB_CV_EXPLAB
(C_CVDNI,T_NOMBRE_EMPRESA,T_ACTIVIDAD_EMPRESA,N_SUELDO,F_FECHA_INI,F_FECHA_FIN)
VALUES(#cvDni#,#nomEmpresa#,#actividadEmpresa#,#sueldo#,#fechaIni#,#fechaFin#)
</insert>

<delete id="deleteCVExpLab" >
DELETE QPDATAGESTION.TB_CV_EXPLAB WHERE C_CVDNI=#cvDni#
</delete>

<select id="selectCVExpLab" resultMap="rsCVExpLab">
SELECT C_CVDNI,T_NOMBRE_EMPRESA,T_ACTIVIDAD_EMPRESA,N_SUELDO,F_FECHA_INI,F_FECHA_FIN
FROM QPDATAGESTION.TB_CV_EXPLAB WHERE C_CVDNI=#cvDni# 
</select>

</sqlMap>