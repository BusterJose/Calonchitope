<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="Nacionalidad" >

	<typeAlias alias="NacionalidadDTO" type="pe.edu.unmsm.quipucamayoc.sgfqRRHH.negocio.dto.NacionalidadDTO"/>
	
	<resultMap class="NacionalidadDTO" id="rsNacionalidad">
		<result property="nacCod" columnIndex="1"/>
		<result property="nacNom" columnIndex="2"/>

	</resultMap>
	
	<select id="selectNacionalidad" resultMap="rsNacionalidad">
		SELECT 
			C_NACCOD, T_NACNOM 
		FROM 
			QPDATAGESTION.TB_NACIONALIDAD
	</select>

</sqlMap>
