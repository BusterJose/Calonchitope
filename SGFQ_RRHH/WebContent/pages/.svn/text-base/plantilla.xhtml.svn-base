<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
	  xmlns:h="http://java.sun.com/jsf/html"
	  xmlns:f="http://java.sun.com/jsf/core"
	  xmlns:p="http://primefaces.prime.com.tr/ui"
	  xmlns:ui="http://java.sun.com/jsf/facelets">
<h:head>
<meta http-equiv="Content-Type" content="text/html; charset=ISO-8859-1" />
<link rel="stylesheet" type="text/css" href="#{loginController.rutaRaiz}/estilos/estilos.css" />
<title><ui:insert name="title">titulo en construccion</ui:insert></title>
</h:head>
<body background="#{loginController.rutaRaiz}/pages/images/fondo_login2.png" 
style="background-repeat: no-repeat; background-position: absolute;" TOPMARGIN="0">

<p:idleMonitor onactive="sessionTimeOutDialog.show();" timeout="1800000" />
    	<p:dialog header="Sesión caducada?" widgetVar="sessionTimeOutDialog" modal="true" fixedCenter="true" closable="false" width="400" visible="false" closeOnEscape="false">
       		<h:outputText value="La sesión a caducado, por favor, pulse sobre el siguiente botón para volver a acceder a la aplicación" />
            <br/>
            <div align="center">
            	 <input id="confirmBackToLogin" type="submit" value="Volver a acceder a la aplicación" 
                 onclick="window.location='#{facesContext.externalContext.requestContextPath}/pages/inicio.jsf';">
                 	
                 </input> 
            	<!-- <h:commandLink id="confirmBackToLogin" actionListener="#{loginController.getCierre}" 
            			onclick="window.location='#{facesContext.externalContext.requestContextPath}/pages/inicio.jsf';"  
                	 value="Volver a acceder a la aplicación" oncomplete="sessionTimeOutDialog.hide();"/> -->
            </div>
        </p:dialog>
<ui:include src="/common/menu.xhtml"></ui:include>
<ui:insert name="contenido">
        <span>Pagina en construccion</span>
        
       
</ui:insert>
</body>
</html>
