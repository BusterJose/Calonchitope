			<from-outcome>publicaciones</from-outcome>
			<to-view-id>/pages/convocatorias/publicarConvocatorias.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>logueoCorrecto</from-outcome>
			<to-view-id>/pages/pagePrincipal.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/pages/convocatorias/publicarConvocatorias.xhtml</from-view-id>
