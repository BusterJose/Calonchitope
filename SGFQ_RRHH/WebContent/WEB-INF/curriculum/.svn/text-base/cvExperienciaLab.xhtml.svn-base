<ui:composition xmlns="http://www.w3.org/1999/xhtml"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:f="http://java.sun.com/jsf/core"	
	xmlns:p="http://primefaces.prime.com.tr/ui">

	 <p:fieldset styleClass="ui-state-hover">
	        <p:dataTable id="cvtrabs" value="#{enviarCVController.cv.expLabs}" var="explab">
	        <p:column>
	        <f:facet name="header"><h:outputText value="Empresa"/>
	        </f:facet>
	        <h:outputText value="#{explab.nomEmpresa}"/>
	        </p:column>
	        <p:column>
	        <f:facet name="header"><h:outputText value="Sueldos"/>
	        </f:facet>
	        <h:outputText value="#{explab.sueldo}"/>
	        </p:column>
	        <p:column>
	        <f:facet name="header"><h:outputText value="Actividad de la Emp."/>
	        </f:facet>
	        <h:outputText value="#{explab.actividadEmpresa}"/>
	        </p:column>
	        <p:column>
	        <f:facet name="header"><h:outputText value="Fecha Inicio"/>
	        </f:facet>
	        <h:outputText value="#{explab.cvfechaIni}"/>
	        </p:column>
	        <p:column>
	        <f:facet name="header"><h:outputText value="Fecha Fin"/>
	        </f:facet>
	        <h:outputText value="#{explab.cvfechaFin}"/>
	        </p:column>
	        </p:dataTable>
	        </p:fieldset>
	        
</ui:composition>